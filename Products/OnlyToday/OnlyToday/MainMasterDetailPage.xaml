<?xml version="1.0" encoding="utf-8" ?>
<MasterDetailPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="OnlyToday.MainMasterDetailPage"
             xmlns:local="clr-namespace:OnlyToday"
             xmlns:pages="clr-namespace:OnlyToday.Pages">
  <MasterDetailPage.Master>
    <local:MainMasterDetailPageMaster x:Name="xMasterPage" />
  </MasterDetailPage.Master>
  <MasterDetailPage.Detail>
    <NavigationPage x:Name="xDetailPage">
      <x:Arguments>
        <pages:HomeTabbedPage />
      </x:Arguments>
    </NavigationPage>
  </MasterDetailPage.Detail>
</MasterDetailPage>